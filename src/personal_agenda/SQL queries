create database personal_agenda;
create table expenses(id int(10) auto_increment primary key, object varchar(40), price double(10,2), month varchar(40));
create table monthly_budgets( id int(10) auto_increment primary key, month varchar(15), total double(10,2), savings double(8,2));
create table appoitnments(id int(10) auto_increment primary key, location varchar(50), description varchar(100), due_date varchar(50));
